## Alias

alias ls='ls -F'
alias lsa='ls -AF'
alias ll='ls -lF'
alias lla='ls -AlF'
alias llt='ls -tlF'
alias sl='ls'
alias ..='cd ../'
alias ..2='cd ../..'
alias ..3='cd ../../..'
alias ..4='cd ../../../..'
alias tm='tmux'
alias tms='tmux ls'
alias tma='tmux attach'
alias g='git'
alias gst='git status'
alias gd='git diff'

## Editor
export EDITOR=$(which vi)

## Lang
export LANG=en_US.UTF-8
